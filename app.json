{
  "expo": {
    "name": "galio-native-kit",
    "description": "Build amazing apps in one codebase. //wip from sdk30 to sdk33",
    "slug": "galio-native-kit",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FE2472"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/galio-org/galio",
    "packagerOpts": {
      "assetExts": ["ttf"]
    }
  }
}